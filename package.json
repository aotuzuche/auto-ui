{
  "name": "auto-ui",
  "version": "0.1.8",
  "description": "auto ui components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development PACKAGE=development node scripts/start.js",
    "build": "cross-env NODE_ENV=production PACKAGE=production && node scripts/build.js",
    "docs": "cross-env NODE_ENV=development PACKAGE=development webpack-dev-server --host 0.0.0.0 --progress --config build/docs.dev.js",
    "docs:build": "rimraf docs/dist && cross-env NODE_ENV=production PACKAGE=production webpack --config build/docs.build.js && gh-pages -d docs/dist",
    "release": "sh scripts/release.sh",
    "lint": "eslint packages --fix --ext .js,.jsx",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git-cz"
  },
  "repository": "git@github.com:aotuzuche/auto-ui.git",
  "author": "shaodahong <337097324@qq.com>",
  "license": "MIT",
  "files": [
    "es",
    "lib",
    "packages"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  },
  "dependencies": {
    "auto-libs": "^0.0.6",
    "classnames": "^2.2.6",
    "iscroll": "^5.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
